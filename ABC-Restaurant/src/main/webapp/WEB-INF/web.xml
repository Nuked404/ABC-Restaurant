<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>BranchController</servlet-name>
  	<display-name>BranchController</display-name>
  	<description></description>
  	<servlet-class>com.abc.controller.BranchController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DashboardController</servlet-name>
  	<display-name>DashboardController</display-name>
  	<description></description>
  	<servlet-class>com.abc.controller.DashboardController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>BranchController</servlet-name>
  	<url-pattern>/Branch</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DashboardController</servlet-name>
  	<url-pattern>/AdminDashboard</url-pattern>
  </servlet-mapping>
</web-app>
